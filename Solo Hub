   end,
})


local Button = Tab2:CreateButton({
   Name = "OniHub Use For Boost",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/Phyoue/Wlymalfrr/main/Wlymalfrr"))()
   -- The function that takes place when the button is pressed
   end,
})


local Button = Tab2:CreateButton({
   Name = "Auto Spin",
   Callback = function()
      getgenv().autospin = true
getgenv().speed = 0.5
 
local targetclan = {
    "Kamado",
    "Agatsuma",
    "Uzui",
    "Rengoku",
    "Kamado ",
    "Agatsuma ",
    "Uzui ",
    "Rengoku "
}

while getgenv().autospin do
    local playerClan = game:GetService("ReplicatedStorage").Player_Data[game.Players.LocalPlayer.Name].Clan.Value
    if table.find(targetclan, playerClan) then
        getgenv().autospin = false
    else
        local args = {[1] = "check_can_spin"}
        game:GetService("ReplicatedStorage").Remotes.To_Server.Handle_Initiate_S:FireServer(unpack(args))
        wait(getgenv().speed)
    end
end
   -- The function that takes place when the button is pressed
   end,
})


local Button = Tab2:CreateButton({
   Name = "Stop Auto Spin",
   Callback = function()
      getgenv().autospin = false
getgenv().speed = 0.5
 
local targetclan = {
    "Kamado",
    "Agatsuma",
    "Uzui",
    "Rengoku",
    "Kamado ",
    "Agatsuma ",
    "Uzui ",
    "Rengoku "
}

while getgenv().autospin do
    local playerClan = game:GetService("ReplicatedStorage").Player_Data[game.Players.LocalPlayer.Name].Clan.Value
    if table.find(targetclan, playerClan) then
        getgenv().autospin = false
    else
        local args = {[1] = "check_can_spin"}
        game:GetService("ReplicatedStorage").Remotes.To_Server.Handle_Initiate_S:FireServer(unpack(args))
        wait(getgenv().speed)
    end
end
   -- The function that takes place when the button is pressed
   end,
})


local Button = Tab2:CreateButton({
   Name = "Auto Daily Spin",
   Callback = function()
      while true do
         wait()
         gameGetService(ReplicatedStorage)WaitForChild(spins_thing_remote)InvokeServer()
     end
   -- The function that takes place when the button is pressed
   end,
})


